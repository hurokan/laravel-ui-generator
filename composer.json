{
    "name": "laravel-ui-generator-vendor/laravel-ui-generator",
    "description": "This is the simplest Laravel package for creating user interfaces",
    "license": "MIT",
    "authors": [
        {
            "name": "Hasan Uddin Rokan",
            "email": "hurokan91@gmail.com",
            "homepage": "http://rokan.bio/"
        }
    ],
    "homepage": "https://github.com/laravel-ui-generator-vendor/laravel-ui-generator",
    "keywords": ["Laravel", "LaravelUiGenerator"],
    "require": {
        "illuminate/support": "^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "orchestra/testbench": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "LaravelUiGeneratorVendor\\LaravelUiGenerator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaravelUiGeneratorVendor\\LaravelUiGenerator\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelUiGeneratorVendor\\LaravelUiGenerator\\LaravelUiGeneratorServiceProvider"
            ],
            "aliases": {
                "LaravelUiGenerator": "LaravelUiGeneratorVendor\\LaravelUiGenerator\\Facades\\LaravelUiGenerator"
            }
        }
    }
}

